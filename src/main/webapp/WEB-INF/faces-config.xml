<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <description>Users Ranking</description>
            <from-outcome>ranking</from-outcome>
            <to-view-id>/public/pages/ranking.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <description>Index Game</description>
            <from-outcome>game</from-outcome>
            <to-view-id>/public/pages/game.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>